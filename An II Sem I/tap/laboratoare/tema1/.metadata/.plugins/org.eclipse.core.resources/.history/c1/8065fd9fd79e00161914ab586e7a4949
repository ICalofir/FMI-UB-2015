package myQueue;

import java.util.*;

public class myQueue<E> implements Iterable<E> {
	LinkedList<E> elems;
	
	myQueue() {
		elems = new LinkedList<E>();
	}
	
	public Iterator<E> iterator() {
		return new myQueueIterator();
	}
	
	private class MyQueueIterator implements Iterator<E> {
		public boolean hasNext() {
			return (elems.size() > 0);
		}
		
	}
}